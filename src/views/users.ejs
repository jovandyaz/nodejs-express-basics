<%- include('partials/header') %>

<h1><%= title %></h1>

<% if(!!users.length) { %>
    <% for(let user of users) { %>
        <p><%= user.name %></p>
        <p><%= user.age %></p>
    <% } %>
<% } %>

<% if(!!posts.length) { %>
    <% for(let post of posts) { %>
        <h2><%= post.title %></h2>
        <article><%= post.body %></article>
    <% } %>
<% } %>

<%- include('partials/footer') %>
